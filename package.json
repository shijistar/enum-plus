{
  "name": "enum-plus",
  "version": "3.0.0-alpha.0",
  "description": "A drop-in replacement for native enum. Like native enum but much better!",
  "keywords": [
    "enum",
    "enumeration",
    "tool",
    "javascript",
    "typescript",
    "frontend",
    "ui binding",
    "ui integration",
    "node.js",
    "ssr",
    "localization",
    "globalization",
    "internationalization",
    "plugin system",
    "react",
    "vue",
    "angular",
    "ant-design",
    "arco-design",
    "material-ui",
    "vuetify",
    "ng-zorro"
  ],
  "homepage": "https://github.com/shijistar/enum-plus",
  "bugs": {
    "url": "https://github.com/shijistar/enum-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shijistar/enum-plus.git"
  },
  "license": "MIT",
  "author": "李凤宝(Leo) <shijistar@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "browser-legacy": "./es-legacy/index.js",
      "default": "./lib/index.js"
    },
    "./enum-collection": {
      "types": "./lib/enum-collection.d.ts",
      "import": "./es/enum-collection.js",
      "require": "./lib/enum-collection.js",
      "browser-legacy": "./es-legacy/enum-collection.js",
      "default": "./lib/enum-collection.js"
    },
    "./enum-item": {
      "types": "./lib/enum-item.d.ts",
      "import": "./es/enum-item.js",
      "require": "./lib/enum-item.js",
      "browser-legacy": "./es-legacy/enum-item.js",
      "default": "./lib/enum-item.js"
    },
    "./es-legacy": {
      "types": "./es-legacy/index.d.ts",
      "import": "./es-legacy/index.js",
      "require": "./es-legacy/index.js",
      "default": "./es-legacy/index.js"
    },
    "./types": "./lib/types.d.ts",
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "es-legacy/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "es-legacy",
    "umd",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "README.zh-CN.md"
  ],
  "workspaces": [
    ".",
    "packages/*"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:es": "run-s task:build-es task:add-umd-banner task:add-es-extensions",
    "build:es-legacy": "cross-env LEGACY=1 run-s task:build-es-legacy task:add-umd-banner task:add-es-legacy-extensions",
    "build:lib": "run-s ts2lib task:copy-dts task:copy-lib",
    "e2e": "run-s task:bundle-e2e task:run-e2e",
    "e2e:debug": "run-s task:bundle-e2e task:run-e2e-debug",
    "e2e:ui": "run-s task:bundle-e2e task:run-e2e-ui",
    "prepare": "husky",
    "prepare-legacy-node14-15": "node ./scripts/prepare-legacy.js node14-15",
    "prepare-legacy-node7-13": "node ./scripts/prepare-legacy.js node7-13",
    "prepublishOnly": "run-s build test",
    "task:add-es-extensions": "tsx scripts/add-esm-extensions.ts es",
    "task:add-es-legacy-extensions": "tsx scripts/add-esm-extensions.ts es-legacy",
    "task:add-umd-banner": "tsx scripts/add-umd-banner.ts",
    "task:build-es": "father build",
    "task:build-es-legacy": "father build",
    "task:bundle-e2e": "tsx scripts/make-e2e-bundle.ts",
    "task:copy-dts": "shx cp ./src/*.d.ts ./lib",
    "task:copy-lib": "tsx scripts/copy-lib.ts",
    "task:jest": "jest --coverage",
    "task:jest:legacy": "jest --coverage -c jest-legacy.config.js",
    "task:run-e2e": "playwright test",
    "task:run-e2e-debug": "playwright test --debug",
    "task:run-e2e-ui": "playwright test --ui",
    "test": "npm run task:jest",
    "test-legacy": "run-s ts2lib task:jest:legacy",
    "ts2lib": "tsc -p tsconfig.lib.json"
  },
  "overrides": {
    "@umijs/babel-preset-umi": {
      "@babel/runtime": "^7.27.1"
    },
    "esbuild": "^0.25.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@swc/core": "^1.11.29",
    "@tiny-codes/code-style-all-in-one": "^1.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "father": "^4.5.5",
    "jest": "^29.7.0",
    "npm-run-all2": "^8.0.3",
    "playwright": "^1.52.0",
    "rollup": "^4.41.1",
    "shx": "^0.4.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "npm@11.4.1",
  "engines": {
    "node": ">=7.10.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
