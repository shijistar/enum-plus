{
  "name": "enum-plus",
  "version": "2.2.7",
  "description": "A drop-in replacement for enum, like native enum but much better!",
  "keywords": [
    "enum",
    "enumeration",
    "typescript",
    "localization",
    "node.js",
    "ssr",
    "tool"
  ],
  "homepage": "https://github.com/shijistar/enum-plus",
  "bugs": {
    "url": "https://github.com/shijistar/enum-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shijistar/enum-plus.git"
  },
  "license": "MIT",
  "author": "李凤宝(Leo) <shijistar@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./es/index.d.ts",
        "default": "./es/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./es/types.d.ts"
      },
      "require": {
        "types": "./lib/types.d.ts"
      }
    },
    "./enum-collection": {
      "import": {
        "types": "./es/enum-collection.d.ts",
        "default": "./es/enum-collection.js"
      },
      "require": {
        "types": "./lib/enum-collection.d.ts",
        "default": "./lib/enum-collection.js"
      }
    },
    "./enum-item": {
      "import": {
        "types": "./es/enum-item.d.ts",
        "default": "./es/enum-item.js"
      },
      "require": {
        "types": "./lib/enum-item.d.ts",
        "default": "./lib/enum-item.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "es-legacy/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "run-p build:es build:es-legacy build:lib",
    "build:es": "father build",
    "build:es-legacy": "cross-env LEGACY=1 father build",
    "build:lib": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest",
    "prepare": "husky"
  },
  "devDependencies": {
    "@swc/core": "^1.3.53",
    "@tiny-codes/code-style-all-in-one": "^1.1.4",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "father": "^4.5.2",
    "jest": "^29.7.0",
    "npm-run-all2": "^7.0.2",
    "ts-jest": "^29.3.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
